{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "rules": {
    "no-tabs": [0],
    "indent": [0],
    "no-console": 0,
    "no-unused-vars": [0, { "varsIgnorePattern": "^h$" }],
    "react/jsx-indent": [0],
    "react/jsx-indent-props": [0],
    "import/prefer-default-export": [1],
    "react/react-in-jsx-scope": [0],
    "react/prop-types": [0],
    "react/no-unknown-property": [2, { "ignore": ["class"] }],
    "jsx-a11y/no-static-element-interactions": [0]
  }
}
